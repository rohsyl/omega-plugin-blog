{
    "name": "rohsyl/omega-plugin-blog",
    "description": "OmegaCMS Plugin Blug",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "rohsyl",
            "email": "syzin12@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.2",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.13.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "rohsyl\\OmegaPlugin\\Blog\\": "src",
            "rohsyl\\OmegaPlugin\\Blog\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rohsyl\\OmegaPlugin\\Blog\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rohsyl\\OmegaPlugin\\Blog\\ServiceProvider"
            ]
        }
    }
}
